#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'mapping==0.0.0','console_scripts','local_mapping_node = mapping.local_mapping:mainlocal_gradient_node'
__requires__ = 'mapping==0.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mapping==0.0.0', 'console_scripts', 'local_mapping_node = mapping.local_mapping:mainlocal_gradient_node')()
    )
